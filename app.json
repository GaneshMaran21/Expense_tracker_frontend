{
  "expo": {
    "name": "Expense_Mobile",
    "slug": "Expense_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "expensemobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Expense-Mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#1d41f9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.Expense_Mobile",
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "./network_security_config.xml"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1d41f9",
          "dark": {
            "backgroundColor": "#0e1b26"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "57653156-0786-4eb2-be7c-650a04641d26"
      }
    }
  }
}
